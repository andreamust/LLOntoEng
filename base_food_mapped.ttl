<http://www.w3.org/TR/2003/PR-owl-guide-20031209/wine> a owl:Ontology ;
    rdfs:label "Wine Ontology" ;
    rdfs:comment "An example OWL ontology",
        """Derived from the DAML Wine ontology at 
      http://ontolingua.stanford.edu/doc/chimaera/ontologies/wines.daml
      Substantially changed, in particular the Region based relations.
    """ ;
    owl:priorVersion <http://www.w3.org/TR/2003/CR-owl-guide-20030818/wine> .

vin:AlsatianWine a owl:Class ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( vin:Wine [ a owl:Restriction ;
                        owl:hasValue vin:AlsaceRegion ;
                        owl:onProperty vin:locatedIn ] ) ] .

vin:AmericanWine a owl:Class ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( vin:Wine [ a owl:Restriction ;
                        owl:hasValue vin:USRegion ;
                        owl:onProperty vin:locatedIn ] ) ] .

vin:BancroftChardonnay a owl:NamedIndividual,
        vin:Chardonnay ;
    vin:hasBody vin:Medium ;
    vin:hasFlavor vin:Moderate ;
    vin:hasMaker vin:Bancroft ;
    vin:hasSugar vin:Dry ;
    vin:locatedIn vin:NapaRegion .

vin:Beringer a owl:NamedIndividual,
        vin:Winery .

vin:CaliforniaWine a owl:Class ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( vin:Wine [ a owl:Restriction ;
                        owl:hasValue vin:CaliforniaRegion ;
                        owl:onProperty vin:locatedIn ] ) ] .

vin:ChateauChevalBlancStEmilion a owl:NamedIndividual,
        vin:StEmilion ;
    vin:hasMaker vin:ChateauChevalBlanc .

vin:ChateauDYchemSauterne a owl:NamedIndividual,
        vin:Sauternes ;
    vin:hasFlavor vin:Strong ;
    vin:hasMaker vin:ChateauDYchem ;
    vin:madeFromGrape vin:SauvignonBlancGrape,
        vin:SemillonGrape .

vin:ChateauDeMeursaultMeursault a owl:NamedIndividual,
        vin:Meursault ;
    vin:hasFlavor vin:Moderate ;
    vin:hasMaker vin:ChateauDeMeursault .

vin:ChateauLafiteRothschildPauillac a owl:NamedIndividual,
        vin:Pauillac ;
    vin:hasMaker vin:ChateauLafiteRothschild .

vin:ChateauMargaux a owl:NamedIndividual,
        vin:Margaux ;
    vin:hasMaker vin:ChateauMargauxWinery .

vin:ChateauMorgonBeaujolais a owl:NamedIndividual,
        vin:Beaujolais ;
    vin:hasMaker vin:ChateauMorgon .

vin:ChiantiClassico a owl:NamedIndividual,
        vin:Chianti ;
    vin:hasBody vin:Medium ;
    vin:hasMaker vin:McGuinnesso .

vin:ClosDeLaPoussieSancerre a owl:NamedIndividual,

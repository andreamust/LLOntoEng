            owl:onProperty :hasDrink ] ;
    owl:equivalentClass [ a owl:Class ;
            owl:intersectionOf ( :MealCourse [ a owl:Restriction ;
                        owl:allValuesFrom :BlandFish ;
                        owl:onProperty :hasFood ] ) ] .

